digraph hierarchy {
  label = "requriements.yml dependency graph of all roles \n Generated by JonasPammer/ansible-roles" ;

  {
{% for role in all_roles %}
    "{{ role.galaxy_role_name }}" [fillcolor={{role.get_dependency_layer_color()}} style=filled]
{% endfor %}
  }

{% for role in all_roles %}
{%-   if role.computed_dependencies|length > 0 %}
  "{{ role.galaxy_role_name }}" -> {% raw %}{"{% endraw %}{{ '" "'.join(role.computed_dependencies) }}{% raw %}"}{% endraw %}
{%-   endif -%}
{% endfor %}
}