digraph hierarchy {
  label = "requirements.yml dependency graph of all roles \n Generated by JonasPammer/ansible-roles" ;

  {
{%- for galaxy_role_name, role in all_roles.items() %}
    "{{ galaxy_role_name }}" [fillcolor={{role.get_dependency_layer_color()}} style=filled]
{%- endfor %}
  }

{% for galaxy_role_name, role in all_roles.items() %}
{%-   if role.computed_dependencies|length > 0 %}
  "{{ galaxy_role_name }}" -> {% raw %}{"{% endraw %}{{ '" "'.join(role.computed_dependencies) }}{% raw %}"}{% endraw %}
{%-   endif -%}
{%- endfor %}
}